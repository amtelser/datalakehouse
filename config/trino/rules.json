{
  "catalogs": [
    { "user": "ANONYMOUS", "catalog": ".*",   "allow": "none" },
    { "user": "admin",     "catalog": ".*",   "allow": "all"  },
    { "user": "analyst",   "catalog": "nessie","allow": "read-only" },
    { "user": "cleanup",   "catalog": "nessie","allow": "all" },
    { "user": "hardware",  "catalog": "nessie","allow": "read-only" }
  ],
  "schemas": [
    { "user": "admin", "catalog": ".*", "schema": ".*", "owner": true }
  ],
  "tables": [
    {
      "user": "admin",
      "catalog": ".*",
      "schema": ".*",
      "table": ".*",
      "privileges": ["SELECT","INSERT","DELETE","UPDATE","OWNERSHIP"]
    },
    {
      "user": "analyst",
      "catalog": "nessie",
      "schema": "telematics",
      "table": ".*",
      "privileges": ["SELECT"]
    },
    {
      "user": "cleanup",
      "catalog": "nessie",
      "schema": "telematics",
      "table": ".*",
      "privileges": ["SELECT","INSERT","DELETE","UPDATE"]
    },
    {
      "user": "hardware",
      "catalog": "nessie",
      "schema": "telematics",
      "table": ".*",
      "privileges": ["SELECT"]
    }
  ]
}